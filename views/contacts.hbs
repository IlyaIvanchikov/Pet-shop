<div class="contact">
      <h1 class="center-align">Пункты самовывоза</h1>
      {{#if contact.length}}
      {{#each contact}}
      <div class="row">
        <div class="col s3 m3">
          <div class="card blue-grey darken-1">
            <div class="card-content white-text">
              <span class="card-title">{{city}}</span>
              <p>{{address}}</p>
            </div>
            <div class="card-action">
             <a href="#">{{number}}</a>
            </div>
          </div>
        </div>
        {{/each}}
      </div>
      {{/if}}
</div>            